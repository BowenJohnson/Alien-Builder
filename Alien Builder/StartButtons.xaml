<Page x:Class="Alien_Builder.StartButtons"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Alien_Builder"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="StartButtons">

    <Grid>
        <StackPanel  Orientation="Horizontal">
            <Button x:Name="btnStart" BorderBrush="{x:Null}" Background="{x:Null}" Margin="10,5,0,0" Width="90" Height="20" HorizontalAlignment="Left" VerticalAlignment="Center" />
        </StackPanel>
    </Grid>
</Page>
